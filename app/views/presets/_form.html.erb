<%= simple_form_for [:api, @preset], html: {class: 'form-horizontal'} do |f| %>
  <%= render "/shared/error_messages", f: f %>
  <%= f.input :name %>
  <%= f.input :parameters %>
  <%= f.input :weight, input_html: { min: 0, max: 10 } %>
  <%= f.button :wrapped, :cancel => presets_path %>
<% end %>

